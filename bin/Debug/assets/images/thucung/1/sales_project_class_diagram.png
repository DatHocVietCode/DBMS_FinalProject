// Sales Project Class Diagram
digraph ClassDiagram {
	rankdir=BT
	User [label="User|String Id\lString email\lString password\lString name\lString phoneNumber\lString address\lint sex\lDate birthDay\l" shape=record]
	Role [label="Role|String id\lString name\lString description\l" shape=record]
	Cart [label="Cart|String id\l" shape=record]
	CartItem [label="CartItem|String id\lint quantity\l" shape=record]
	Order [label="Order|String id\ldouble total\lDate date\lString note\lString status\l" shape=record]
	OrderDetail [label="OrderDetail|String id\lint quantity\ldouble price\l" shape=record]
	Product [label="Product|String id\lString name\lString description\l" shape=record]
	ProductItem [label="ProductItem|String id\ldouble price\lint quantity\lboolean status\lString image\l" shape=record]
	Size [label="Size|String id\lString name\lString description\l" shape=record]
	Color [label="Color|String id\lString name\lString description\l" shape=record]
	Category [label="Category|String id\lString name\lString description\l" shape=record]
	User -> Role [label="Many-to-One"]
	User -> Cart [label="One-to-One"]
	User -> Order [label="One-to-Many"]
	Cart -> CartItem [label="One-to-Many"]
	CartItem -> ProductItem [label="Many-to-One"]
	Order -> OrderDetail [label="One-to-Many"]
	OrderDetail -> ProductItem [label="Many-to-One"]
	ProductItem -> Size [label="Many-to-One"]
	ProductItem -> Color [label="Many-to-One"]
	ProductItem -> Product [label="Many-to-One"]
	Product -> Category [label="Many-to-One"]
	Size -> ProductItem [label="One-to-Many"]
	Color -> ProductItem [label="One-to-Many"]
	Category -> Product [label="One-to-Many"]
}
